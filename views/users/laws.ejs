<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Yucky Politicians | Laws</title>
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/laws.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
</head>

<body>
    <%- include('../partials/flash')%>

        <div>
            <h1>Laws</h1>
            <ul>
                <% user.laws.forEach(function(result){ %>
                    <li>
                        <a
                            href="/law/<%= result.requestData.congress %>/<%= result.requestData.lawType %>/<%= result.requestData.lawNumber %>">
                            <%= result.lawData.title %>
                        </a>
                    </li>
                    <% }); %>
            </ul>
        </div>
</body>

</html>